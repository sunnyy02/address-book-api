{
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["./src/**/*.ts", "!./dist/**", "!**/*.(dto|entity|enum).ts", "!**/*.module.ts", "!*.module.ts"],
  "coverageDirectory": "./coverage-e2e",
  "collectCoverage": true
}
